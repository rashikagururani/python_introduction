x=int(input('Enter 1st odd no. :'))
y=int(input('Enter 2nd odd no. :'))
z=int(input('Enter 3rd odd no. :'))

if(x>y and x>z and x%2!=0):
    print('Largest odd no. is:',x)
elif(y>z and y%2!=0):
    print('Largest odd no. is:',y)
elif(z%2!=0):
    print('Largest odd no. is:',z)
else:
    print('You have entered wrong values.Booo!!')
